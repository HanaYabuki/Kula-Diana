{
    "name": "kula-diana",
    "displayName": "Kula - Diana",
    "publisher": "HanaYabuki",
    "description": "Kula (lang) High-light for VSCode",
    "keywords": [
        "kula",
        "kula-lang",
        "kula-programming-language",
        "kof",
        "diana"
    ],
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/kula.png",
    "activationEvents": [],
    "main": "./src/extension",
    "contributes": {
        "commands": [],
        "languages": [
            {
                "id": "kula",
                "icon": {
                    "dark": "./images/ico_light.svg",
                    "light": "./images/ico_dark.svg"
                },
                "aliases": [
                    "Kula",
                    "kula"
                ],
                "extensions": [
                    ".kula"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "kula",
                "scopeName": "source.kula",
                "path": "./syntaxes/kula.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "directory": "https://github.com/kula-lang/kula-diana"
    }
}