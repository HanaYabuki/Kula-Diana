{
    "name": "kula-diana",
    "displayName": "Kula - Diana",
    "publisher": "HanaYabuki",
    "description": "Kula (lang) High-light for VSCode",
    "keywords": ["kula", "kof"],
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/kula.png",
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "hello"
            }
        ],
        "languages": [{
            "id": "kula",
            "aliases": ["Kula", "kula"],
            "extensions": [".kula"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "kula",
            "scopeName": "source.kula",
            "path": "./syntaxes/kula.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "directory": "https://github.com/HanaYabuki/Kula-Diana"
    }
}