{
    "name": "kula-diana",
    "displayName": "kula-diana",
    "description": "Kula for VSCode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./src/extension.js",
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "hello"
            }
        ],
        "languages": [{
            "id": "kula",
            "aliases": ["Kula", "kula"],
            "extensions": [".kula"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "kula",
            "scopeName": "source.kula",
            "path": "./syntaxes/kula.tmLanguage.json"
        }]
    }
}